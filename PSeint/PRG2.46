Algoritmo sin_titulo
	preu_total=0
	Repetir
		Escribir "Quants productes vas a comprar"
		Leer num_prod
		Escribir "Quin preu te el producte que vas a comprar"
		Leer preu_prod
		preu_total=preu_total+num_prod*preu_prod
	Hasta Que num_prod=0
	preu_iva=preu_total*(16/100)
	preu_tot_iva=preu_total+preu_iva
	si preu_tot_iva>300
		desc=preu_tot_iva*(5/100)
		preu_tot_iva=preu_tot_iva-desc
	FinSi
	Escribir "Limport a pagar es de: " preu_tot_iva
FinAlgoritmo
