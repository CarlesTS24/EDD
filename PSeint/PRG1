10

	(AA - AN > 18) or ((AA - AN = 18) and (MN < MA)) or ((AA - AN = 18) and (MA == MN) and (DN <= DA))


	
11

a)
	(nl + nv + nm + nf) / 4 > 5
b)
	(nm > (nl + nv + nm + nf) / 4) and (nv > (nl + nv + nm + nf) / 4)
c)
	(nl > 5) or (nv > 5) or (nm > 5) or (nf >5)
d)
	(nl > 5) and (nv > 5) and (nm > 5) and (nf >5)
e)
	((nl + nv + nm + nf) / 4 >= 5) and ((opcion == "L" and (nl + nv) / 2 >= 5) or (opcion == "C" and (nm + nf) / 2 >= 5))
