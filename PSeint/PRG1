8

a)
	(b > d) and (c > d)
b)
	(a == b) and (b == c)
c)
	(a == b) and (b == c) and (c != d)
d)
	(b >= a) and (b <= c) or NOT ((b >= a) and (b <= c))
e)
	(a < c) and (b >= a) and (b <= c) 
f)
	(a == b)  or  (a == c)  or  (b == c)
g)
	((a == b) and (a != c)) or ((b != a) and (b == c)) or ((c == a) and (c != b))
h)
	NOT (a==b and b==c)
	a!=b or b!=c
	


